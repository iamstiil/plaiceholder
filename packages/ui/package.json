{
  "private": true,
  "name": "@plaiceholder/ui",
  "description": "Private UI components for plaiceholder examples",
  "author": "Joe Bell <joe@joebell.co.uk>",
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
